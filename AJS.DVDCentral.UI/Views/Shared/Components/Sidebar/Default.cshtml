@model IEnumerable<AJS.DVDCentral.BL.Models.Genre>

<ul>
    @foreach (var program in Model)
    {
        <li>
            <a asp-controller="Movie" asp-action="Browse"
               asp-route-id="@program.Id"> @program.Description</a>
        </li>
    }

    <li><a asp-controller="Movie" asp-action="Index">All</a></li>

</ul>